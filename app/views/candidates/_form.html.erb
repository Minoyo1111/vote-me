
<%= form_for(@candidate) do |f| %>
  <%= f.label :name, "姓名" %>
  <%= f.text_field :name %> <br />

  <%= f.label :name, "年齡" %>
  <%= f.text_field :age %> <br />

  <%= f.label :name, "政黨" %>
  <%= f.text_field :party %> <br />

  <%= f.label :name, "政見" %>
  <%= f.text_area :politics %> <br />

  <%= f.submit %>

<% end %>



